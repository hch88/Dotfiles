s=$(command -V deactivate_lua 2>&1)
if [ $? -eq 0 ]; then
    if [ "${s##*function*}" = '' ]; then
        deactivate_lua
    fi;
fi;

deactivate_lua () {
    if [ -x '/home/harish/Dotfiles/awesome/.config/awesome/install/bin/lua' ]; then
        PATH=`'/home/harish/Dotfiles/awesome/.config/awesome/install/bin/lua' '/home/harish/Dotfiles/awesome/.config/awesome/install/bin/get_deactivated_path.lua'`
        export PATH

        hash -r 2>/dev/null
    fi

    unset -f deactivate_lua
}

PATH='/home/harish/Dotfiles/awesome/.config/awesome/install/bin':"$PATH"
export PATH

hash -r 2>/dev/null
